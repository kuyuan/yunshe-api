{
  "name": "yunshe-core-api",
  "version": "1.0.0",
  "description": "Core api for community yunshe",
  "scripts": {
    "dev": "backpack",
    "build": "backpack build",
    "start": "node ./build/main",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "yarn lint --fix",
    "test": "jest",
    "deploy": "prisma deploy --env-file .env && prisma deploy --env-file .env.test",
    "deploy:test": "prisma deploy --env-file .env.test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn fix"
    }
  },
  "author": "Branliang",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "5.4.2",
    "apollo-errors": "^1.9.0",
    "cookie-session": "^1.3.3",
    "cos-nodejs-sdk-v5": "^2.5.6",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "got": "^9.6.0",
    "graphql": "^14.1.1",
    "graphql-date": "^1.0.3",
    "graphql-shield": "^5.3.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.0",
    "keygrip": "^1.0.3",
    "merge-graphql-schemas": "^1.5.8",
    "mongodb": "^3.1.13",
    "passport": "^0.4.0",
    "passport-weixin": "^0.2.0",
    "prisma-client-lib": "^1.34.0",
    "sanitize-filename": "^1.6.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.36",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.6",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongodb": "^3.5.7",
    "@types/passport": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "backpack-core": "^0.8.3",
    "get-port": "^5.0.0",
    "husky": "^2.0.0",
    "jest": "^24.1.0",
    "node-mocks-http": "^1.7.3",
    "prisma": "^1.31.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "typescript": "^3.4.1"
  }
}
