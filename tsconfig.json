{
  "compilerOptions": {
    "lib": [
      "es2016",
      "esnext.asynciterable"
    ],
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@schemaTypes/*": [ "src/types/*" ],
      "@models/*": [ "src/models/*" ],
      "@utils/*": [ "src/utils/*" ],
      "@resolvers/*": [ "src/resolvers/*" ],
      "@loaders/*": ["src/loaders/*"],
      "@support/*": ["support/*"],
      "@prisma/*": ["generated/prisma-client/*"]
    },
    "skipLibCheck": true
  }
}
